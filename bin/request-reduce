#!/bin/bash
awk '{
        owners[$1] = $1;
        total[$1] += $2;
        gets[$1] += $3;
        puts[$1] += $4;
        deletes[$1] += $5;
        heads[$1] += $6;
        posts[$1] += $7;
        bwin[$1] += $8;
        bwout[$1] += $9;
} END {
        for (o in owners) {
                print o, total[o], gets[o], puts[o], deletes[o], heads[o], posts[o], bwin[o], bwout[o];
        }
}'
