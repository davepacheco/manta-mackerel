set -o pipefail

dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

$ZCAT \
| $dir/../lib/storage-map.js 2>stderr #\
#| msplit -j -n $NUM_REDUCERS -f owner,type,objectId 2>>stderr

[ -s stderr ] && mmkdir -p $(dirname $MANTA_OUTPUT_BASE) \
         && mput -f stderr $MANTA_OUTPUT_BASE && cat stderr 1>&2
